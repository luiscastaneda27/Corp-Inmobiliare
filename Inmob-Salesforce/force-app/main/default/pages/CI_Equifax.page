    <script>
    sforce.connection.sessionId = '{!$Api.Session_ID}'; 
    var opId = '{!Opportunity.Id}'; //alert(opId);
    $(document).ready( function () {  
      var opId = '{!Opportunity.Id}'; 
      var TipoIdenti = '{!Opportunity.Account.CI_Tipo_Identi__c}';
      var numeroDocumento = '{!Opportunity.Account.CI_Identificacion__c}';
      var numeroDocumento = '{!Opportunity.Account.CI_Identificacion__c}';
      var Proyecto = '{!Opportunity.Account.CI_Proyecto__r.Name}';
      var Etapa = '{!Opportunity.StageName}'; /*alert(Etapa);*/ if(Etapa == 'Vendida' || Etapa == 'No negociada') { $("[id$='consultarEqx']").attr("disabled","true"); toastLaunch2(); }
      if (TipoIdenti == '2') {
          document.getElementById("TipoIdenti").value = 'C';
      }
      $("[id$='numeroDocumento']").val(numeroDocumento);
      document.getElementById("Proyecto").value = Proyecto;    
      
      if(Proyecto == 'LA RIOJA') { 
        $("[id$='tipoProyecto']").val('LARI');
      } else if(Proyecto == 'LA GRAN VITTORIA') { 
        $("[id$='tipoProyecto']").val('VITT');
      }
         
      //Eliminando registros existentes
      var result = sforce.connection.query("Select id, name, CI_Oportunidad__c, CI_Fecha_corte__c, CI_Institucion__c, CI_Tipo_deudor__c, CI_Total_vencer__c, CI_Total_vencido__c, CI_No_devenga_interes__c, CI_Demanda_judicial__c, CI_Cartera_castigada__c, CI_Dias_vencidos__c from Datos_Equifax__c Where CI_Oportunidad__c = '"+opId+"'");
      var records = result.getArray("records"); 
      var idsForDeletion = [];    
      for (var i=0; i<records.length; i++) {
          idsForDeletion.push(records[i].Id);
      } 
      if(idsForDeletion != null) {
          sforce.connection.deleteIds(idsForDeletion);
      }
      //Fin Eliminando registros existentes          
    } );
    function vecesConsultadas() {
        var idOpor = '{!Opportunity.Id}'; //alert(idOpor);
        recOportunidad = sforce.connection.query("SELECT Id, Name, Consultas_Equifax__c FROM Opportunity WHERE Id='"+idOpor+"'"); //alert(recOportunidad);      
        var recOpor = recOportunidad.getArray('records'); //alert(recOpor);     
        try {
            var updateOpor = new sforce.SObject("Opportunity");
            updateOpor.Id = recOpor[0].Id;
            updateOpor.Consultas_Equifax__c = parseInt(recOpor[0].Consultas_Equifax__c)+1;    
            result = sforce.connection.update([updateOpor]);
            //alert('Datos Actualizados '+result[0].id); 
            //console.log(result);
        } catch(err) {
            alert(err);
        }
    }
    </script>
    
