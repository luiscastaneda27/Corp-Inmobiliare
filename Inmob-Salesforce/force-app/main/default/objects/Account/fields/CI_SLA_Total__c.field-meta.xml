<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CI_SLA_Total__c</fullName>
    <description>Total en días desde que se creó el registro (Prospecto-Cuenta).</description>
    <externalId>false</externalId>
    <formula>IF(
    ISBLANK(CI_Reassignment_date_time__c),
    CI_SLA_Prospecto__c + CI_SLA_Cuenta__c,
    CI_SLA_Cuenta__c
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Total en días desde que se creó el registro (Prospecto-Cuenta).</inlineHelpText>
    <label>SLA Total</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
