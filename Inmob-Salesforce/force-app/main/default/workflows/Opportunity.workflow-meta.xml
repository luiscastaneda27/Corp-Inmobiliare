    <rules>
        <fullName>CI Cambiar Propietario de la Cuenta RIOJA</fullName>
        <actions>
            <name>CI_Cambiar_Propietario_de_la_Cuenta_RIOJA</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Opportunity.CI_Cambia_prop_cuenta__c</field>
            <operation>equals</operation>
            <value>Verdadero</value>
        </criteriaItems>
        <criteriaItems>
            <field>Opportunity.CI_Proyecto__c</field>
            <operation>equals</operation>
            <value>LA RIOJA</value>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
