    <rules>
        <fullName>CI Prospecto %28Lead%29 dar de baja automático RIOJA</fullName>
        <actions>
            <name>CI_Prospecto_Lead_Inactivar_Autom_tico_RIOJA</name>
            <type>Alert</type>
        </actions>
        <actions>
            <name>Dar_de_Baja_Lead</name>
            <type>FieldUpdate</type>
        </actions>
        <actions>
            <name>Motivo_de_Baja_Lead</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <description>El prospecto se dio de baja por falta de gestión.</description>
        <formula>AND(OR(TEXT(Status)!=&apos;Dado de Baja&apos;,TEXT(Status)!= &apos;Qualified &apos;),CI_Proyecto__r.Name=&apos;LA RIOJA&apos;,CI_Dias_sin_gestionar__c=31)</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
